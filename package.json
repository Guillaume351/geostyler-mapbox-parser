{
  "name": "geostyler-mapbox-parser",
  "version": "0.1.0",
  "description": "GeoStyler-Style-Parser implementation for Mapbox",
  "main": "build/dist/MapboxStyleParser.js",
  "types": "build/dist/MapboxStyleParser.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geostyler/geostyler-mapbox-parser.git"
  },
  "files": [
    "build",
    "index.d.ts",
    "browser"
  ],
  "keywords": [
    "mapbox",
    "geostyler"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/geostyler/geostyler-mapbox-parser/issues"
  },
  "homepage": "https://github.com/geostyler/geostyler-mapbox-parser#readme",
  "dependencies": {
    "geostyler-style": "^4.0.3"
  },
  "scripts": {
    "build": "tsc && npm run build:browser",
    "build:browser": "webpack --config browser-build.config.js",
    "start:dev": "webpack --config dev-build.config.js --watch",
    "prebuild": "npm run test",
    "pretest": "npm run lint",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint": "eslint -c .eslintrc.js --ext .ts,tsx src/ && tsc --noEmit --project tsconfig.json",
    "release": "np --no-yarn && git push https://github.com/geostyler/geostyler-mapbox-parser.git master --tags"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@terrestris/eslint-config-typescript": "^2.0.0",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.0",
    "np": "^7.5.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "/src/.*\\.spec.(ts|js)$",
    "collectCoverageFrom": [
      "src/*.{ts,js}"
    ]
  }
}
